<?import aa.monoglot.ui.ControlledTab?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<ControlledTab fx:id="tab" text="%tab.inflection" xmlns:fx="http://javafx.com/fxml"
               fx:controller="aa.monoglot.ui.controller.InflectionTableController">
    <GridPane alignment="CENTER" hgap="10.0" vgap="10.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <Label style="-fx-font: NORMAL 20 Tahoma;" text="Inflection Table"
               GridPane.columnIndex="0" GridPane.rowIndex="0">
        </Label>
        <TableView fx:id="tableView" GridPane.columnIndex="0"
                   GridPane.rowIndex="1">
        </TableView>
    </GridPane>
</ControlledTab>